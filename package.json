{
  "name": "ngx-hammer",
  "version": "1.1.0",
  "description": "Hammer.js wrapper for Angular4+ to support some operation in the mobile",
  "angular-cli": {},
  "main": "dist/ngx-hammer/bundles/ngx-hammer.umd.js",
  "author": "bsdfzzzy@gmail.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 4199",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --prod ngx-hammer",
    "npm:publish": "rm -rf ./dist && npm run build:lib",
    "npm:pack": "cd dist/ngx-hammer && npm pack",
    "package": "npm run build:lib && npm run npm:pack"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsdfzzzy/ngx-hammer.git"
  },
  "dependencies": {
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular-devkit/build-ng-packagr": "^0.7.5",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.1",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
